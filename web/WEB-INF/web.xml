<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletUsuario</servlet-name>
        <servlet-class>ServletUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletJugador</servlet-name>
        <servlet-class>ServletJugador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEstadio</servlet-name>
        <servlet-class>ServletEstadio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletEquipo</servlet-name>
        <servlet-class>ServletEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCiudad</servlet-name>
        <servlet-class>ServletCiudad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPais</servlet-name>
        <servlet-class>ServletPais</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tarea</servlet-name>
        <servlet-class>Tarea</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>ServletLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletUsuario</servlet-name>
        <url-pattern>/ServletUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletJugador</servlet-name>
        <url-pattern>/ServletJugador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEstadio</servlet-name>
        <url-pattern>/ServletEstadio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletEquipo</servlet-name>
        <url-pattern>/ServletEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCiudad</servlet-name>
        <url-pattern>/ServletCiudad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPais</servlet-name>
        <url-pattern>/ServletPais</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tarea</servlet-name>
        <url-pattern>/Tarea</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/ServletLogin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
